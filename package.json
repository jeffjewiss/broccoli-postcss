  "dependencies": {
    "broccoli-persistent-filter": "1.2.13",
    "object-assign": "4.1.1",
    "postcss": "5.2.14"
  },
  "devDependencies": {
    "async": "^2.1.0",
    "broccoli": "^1.1.0",
    "coveralls": "^2.11.15",
    "glob": "^7.1.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "postcss-pseudoelements": "^3.0.0",
