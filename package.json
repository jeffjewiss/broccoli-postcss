{
  "name": "broccoli-postcss",
  "version": "3.2.1",
  "description": "Postcss compiler for Broccoli",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffjewiss/broccoli-postcss"
  },
  "scripts": {
    "test": "nyc mocha",
